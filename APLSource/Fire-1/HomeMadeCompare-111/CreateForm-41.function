 ∆,←⊂'Coord' 'Pixel'
 ∆,←⊂'Size'(400 600)
 ∆,←⊂'Posn'(⍬ ⍬)
 ∆,←⊂'FontObj'##.GUI.n.APL_Font
 :If 9=⎕NC⍕##.GUI.n.TheIcon
     ∆,←⊂'IconObj'##.GUI.n.TheIcon
 :EndIf
